{
  "name": "every-name",
  "version": "0.6.0",
  "description": "A name generator that lets you use combinations of templates to dynamic make complex names ",
  "homepage": "https://github.com/pristinenoise/every-name",
  "author": "James Stuart <james.stuart@gmail.com>",
  "license": "MIT",
  "main": "./dist/index.js",
  "scripts": {
    "build": "./node_modules/.bin/babel -d dist src/",
    "test": "node node_modules/jest/bin/jest.js",
    "test:coverage": "node node_modules/jest/bin/jest.js --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test:watch": "node node_modules/jest/bin/jest.js --watch",
    "prepublish": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pristinenoise/every-name.git"
  },
  "keywords": [
    "name",
    "generator",
    "names",
    "template"
  ],
  "bugs": {
    "url": "https://github.com/pristinenoise/every-name/issues"
  },
  "homepage": "https://github.com/pristinenoise/every-name#readme",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "coveralls": "^2.13.1",
    "jest-cli": "^20.0.4",
    "lodash": "^4.17.4",
    "mustache": "^2.3.0"
  },
  "devDependencies": {
    "jest": "^20.0.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 90,
        "branches": 90,
        "functions": 90,
        "lines": 90
      }
    }
  }
}
