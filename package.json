{
  "name": "every-name",
  "version": "0.7.2",
  "description": "A name generator that lets you use combinations of templates to dynamic make complex names ",
  "homepage": "https://github.com/pristinenoise/every-name#readme",
  "author": "James Stuart <james.stuart@gmail.com>",
  "license": "MIT",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc -p . && rm -fr dist/tests && mv dist/src/** dist/ && rm -rf dist/src",
    "lint": "yarn run eslint --ext=ts,tsx src/**/* __tests__/**/*",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pristinenoise/every-name.git"
  },
  "keywords": [
    "name",
    "generator",
    "names",
    "template"
  ],
  "bugs": {
    "url": "https://github.com/pristinenoise/every-name/issues"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "coveralls": "^3.0.3",
    "lodash": "^4.17.11",
    "mustache": "^3.0.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.22",
    "@types/lodash": "^4.14.146",
    "@types/mustache": "^0.8.32",
    "@types/node": "^12.12.7",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "typescript": "^3.7.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 90,
        "branches": 90,
        "functions": 90,
        "lines": 90
      }
    }
  }
}
