      "display": "Double First Name",
      "ingredients": [
        "{{first_name}} {{first_name}}"
      ]
    },
    "first_name": {
      "only_text": true,
      "display": "First Names",
      "ingredients": [
        "Alice", "Bob", "Charles", "Dave"
      ]
    },
    "last_name": {
      "only_text": true,
