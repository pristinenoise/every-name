  "recipes": {
    "full_name": {
      "display": "Full Names",
      "description": "First and Last Names",
      "ingredients": [
        "{{first_name}} {{last_name}}",
        "{{first_name}} {{last_name}}",
        "{{first_name}} {{last_name}}",
        "{{first_name}} {{last_name}}",
        "{{first_name}} {{last_name}} {{suffix}}"
      ]
